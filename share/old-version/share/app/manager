	314) TMOE_LANG="wae_CH" ;;
	315) TMOE_LANG="wal_ET" ;;
	316) TMOE_LANG="wo_SN" ;;
	317) TMOE_LANG="xh_ZA.UTF-8" ;;
	318) TMOE_LANG="yi_US.UTF-8" ;;
